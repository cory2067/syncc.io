-    else static if (F[0..2] == "%%")
-        enum FormatString = "%" ~ FormatString!(F[2..$],A);
-    else
-    {
-        static assert(F[0] != '%', "unrecognized format %" ~ F[1]);
-        enum FormatString = F[0] ~ FormatString!(F[1..$],A);
-    }
